{
  "name": "api",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "studio": "npx prisma studio",
    "migrate:dev": "npx prisma migrate dev --preview-feature",
    "migrate": "npx prisma migrate deploy --preview-feature",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "NODE_ENV=development ts-node --transpile-only src/schema"
  },
  "engines": {
    "node": "15.0.1"
  },
  "dependencies": {
    "@prisma/cli": "^2.14.0",
    "@prisma/client": "^2.14.0",
    "@types/node-fetch": "^2.5.8",
    "apollo-server": "^2.19.2",
    "graphql": "^15.4.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.28.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1"
  }
}
